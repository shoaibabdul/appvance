    <options type="sahi">
      <opt name="fullPageLoaded" value="true"/>
      <opt name="clearCache" value="true"/>
      <opt name="takeSnapshot" value="false"/>
      <opt name="debugMode" value="false"/>
    </options>
    <options type="seleniumrc">
      <opt name="listenport" value="6543"/>
      <opt name="port" value="5555"/>
    </options>
  </options>
  <templates>
    <controller name="OpsPortal_300U_FF">
      <round name="OpsPortal_300U_FF" totalusers="300" time="30" testtype="users" totaliterations="2400"/>
    </controller>
    <testcase name="TC1_OpsPortal_LotNoSearch_FF" expand="yes">
      <test>
        <run name="Untitled1" testclass="sahi" method="firefox" langtype="ds" resource="TC1_OpsPortal_LotNoSearch_FF.ds">
          <argument name="TC1_TD" method="getNextData" dpl="TC1_TD"/>
        </run>
      </test>
    </testcase>
    <testcase name="TC2_OpsPortal_LotNo_ImgDocSearch_FF" expand="yes">
      <test>
        <run name="Untitled2" testclass="sahi" method="firefox" langtype="ds" resource="TC2_OpsPortal_LotNo_ImgDocSearch_FF.ds">
          <argument name="TC2_TD" method="getNextData" dpl="TC2_TD"/>
        </run>
      </test>
    </testcase>
    <testcase name="TC3_OpsPortal_ExtSearch_YardNo_FF" expand="yes">
      <test>
        <run name="Untitled3" testclass="sahi" method="firefox" langtype="ds" resource="TC3_OpsPortal_ExtSearch_YardNo_FF.ds">
