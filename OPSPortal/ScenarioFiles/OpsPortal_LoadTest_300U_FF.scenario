          <argument name="TC5_TD" method="getNextData" dpl="TC5_TD"/>
        </run>
      </test>
    </testcase>
    <testcase name="TC6_OpsPortal_ExtSearch_AuctionDates_FF" expand="yes">
      <test>
        <run name="Untitled6" testclass="sahi" method="firefox" langtype="ds" resource="TC6_OpsPortal_ExtSearch_AuctionFromToDates_FF.ds">
          <argument name="TC6_TD" method="getNextData" dpl="TC6_TD"/>
