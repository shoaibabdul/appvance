    <resource name="TC2_OpsPortal_LotNo_ImgDocSearch_FF.ds" type="ds" path="REPOSITORY/C3Seller_Perf/OPSPortal/DesignerScripts/TC2_OpsPortal_LotNo_ImgDocSearch_FF.ds" cache="false"/>
